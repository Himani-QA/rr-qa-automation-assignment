{
    "name": "rr-qa-automation-assignment",
    "version": "1.0.0",
    "description": "QA Automation Assignment for Rapyuta Robotics - TMDB Discover Test Suite",
    "main": "index.js",
    "scripts": {
        "test": "playwright test",
        "test:headed": "playwright test --headed",
        "test:debug": "playwright test --debug",
        "test:chromium": "playwright test --project=chromium",
        "test:firefox": "playwright test --project=firefox",
        "test:webkit": "playwright test --project=webkit",
        "test:all-browsers": "playwright test --project=chromium --project=firefox --project=webkit",
        "test:smoke": "playwright test --grep @smoke",
        "test:regression": "playwright test --grep @regression",
        "test:ui": "playwright test --ui",
        "report": "playwright show-report",
        "report:open": "npx playwright show-report playwright-report",
        "allure:generate": "allure generate allure-results --clean -o allure-report",
        "allure:open": "allure open allure-report",
        "allure:serve": "allure serve allure-results",
        "install:browsers": "npx playwright install --with-deps",
        "lint": "eslint tests/ pages/ utils/",
        "lint:fix": "eslint tests/ pages/ utils/ --fix",
        "format": "prettier --write \"**/*.{js,json,md}\"",
        "clean": "rm -rf test-results playwright-report allure-results allure-report logs",
        "pretest": "node -e \"const fs = require('fs'); ['test-results', 'logs', 'test-results/screenshots'].forEach(dir => fs.mkdirSync(dir, { recursive: true }))\"",
        "ci": "npm run clean && npm test -- --reporter=list --reporter=json --reporter=junit",
        "ci:parallel": "npm run clean && npm test -- --workers=4"
    },
    "keywords": [
        "playwright",
        "automation",
        "testing",
        "qa",
        "e2e",
        "tmdb",
        "rapyuta-robotics"
    ],
    "author": "Your Name <your.email@example.com>",
    "license": "MIT",
    "devDependencies": {
        "@playwright/test": "^1.40.1",
        "allure-commandline": "^2.25.0",
        "allure-playwright": "^2.15.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-playwright": "^0.21.0",
        "prettier": "^3.1.1"
    },
    "dependencies": {
        "winston": "^3.11.0",
        "dotenv": "^16.3.1"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/rr-qa-automation-assignment.git"
    },
    "bugs": {
        "url": "https://github.com/yourusername/rr-qa-automation-assignment/issues"
    },
    "homepage": "https://github.com/yourusername/rr-qa-automation-assignment#readme"
}